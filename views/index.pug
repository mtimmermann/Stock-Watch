extends layout

block content

  section(class='container')
    div(class='row')
      div(class='col-lg-12')
        div#container-chart
    
    div(class='row')
      div(class='stocks-container')
        - var classes = ['col-md-4', 'col-sm-6', 'stock-block', 'stock-new']
        div(class=classes id='stock-add-form')

  script(src='/js/dependencies/highcharts/highstock.js')
  script(src='/js/dependencies/highcharts/exporting.js')
  script(src='/js/scripts.js')
  script(src='/js/templates/templates.js')
